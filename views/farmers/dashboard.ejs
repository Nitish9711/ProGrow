                                <h2>Stocks</h2>
                                <ul>
                                    <% for(const stock of stocks){ %>
                                        <li>
                                            Name: <%=stock.name%>
                                            <br>
                                            Category: <%=stock.category%>
                                            <br>
                                            Qty: <%=stock.qty.amount%> <%=stock.qty.unit%>
                                            <br>
                                            Price: <%=stock.price%> / <%=stock.qty.unit%>
                                        </li>
                                    <% } %>
                                </ul>
                                <h2>Lands</h2>
                                <ul>
                                    <% for(const land of lands){ %>
                                        <li>
                                            Soil Type: <%=land.soilType%>
                                            <br>
                                            Area: <%=land.area.amount%> <%=land.area.unit%>
                                            <br>
                                            Address: <%=land.address.village%>
                                            <br>
                                            <%=land.address.district%>
                                            <br>
                                            <%=land.address.state%>
                                        </li>
                                    <% } %>
